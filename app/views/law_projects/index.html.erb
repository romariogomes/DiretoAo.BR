<p id="notice"><%= notice %></p>

<h1>Law Projects</h1>

<table>
  <thead>
    <tr>
      <th>Law number</th>
      <th>Description</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @law_projects.each do |law_project| %>
      <tr>
        <td><%= law_project.law_number %></td>
        <td><%= law_project.description %></td>
        <td><%= law_project.date %></td>
        <td><%= link_to 'Show', law_project %></td>
        <td><%= link_to 'Edit', edit_law_project_path(law_project) %></td>
        <td><%= link_to 'Destroy', law_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Law Project', new_law_project_path %>
